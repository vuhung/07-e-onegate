<?xml version="1.0" encoding="UTF-8"?>

<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0" xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd">
	<portlet>
		<portlet-name>vdocmanage</portlet-name>
		<display-name>Quản Trị Thủ tục hành chính</display-name>
		<portlet-class>com.vsi.vdoc.portlet.VDocManage</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/vdoc_manage/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/vdoc_manage/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Quản Trị Thủ tục hành chính</title>
			<short-title>VDocManage</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>vdocmenu</portlet-name>
		<display-name>Menu Thủ tục hành chính</display-name>
		<portlet-class>com.vsi.vdoc.portlet.VDocMenu</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/vdocmenu/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/vdocmenu/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Menu Thủ tục hành chính</title>
			<short-title>VDocMenu</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-publishing-event>
			<qname xmlns:x="http://liferay.com/events">x:ipc.vdoc</qname>
		</supported-publishing-event>
	</portlet>
	
	<portlet>
		<portlet-name>vdocview</portlet-name>
		<display-name>Nội dung Thủ tục hành chính</display-name>
		<portlet-class>com.vsi.vdoc.portlet.VDocContent</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/vdocview/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/vdocview/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Nội dung Thủ tục hành chính</title>
			<short-title>VDocView</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
		<supported-processing-event>
			<qname xmlns:x="http://liferay.com/events">x:ipc.vdoc</qname>
		</supported-processing-event>
	</portlet>
	<event-definition>
		<qname xmlns:x="http://liferay.com/events">x:ipc.vdoc</qname>
		<value-type>java.lang.Long</value-type>
	</event-definition>
</portlet-app>