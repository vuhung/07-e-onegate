			/js/main.js
		</footer-portlet-javascript>
		<css-class-wrapper>vlegal-portlet</css-class-wrapper>
	</portlet>
	
	<portlet>
		<portlet-name>viewcontentaction</portlet-name>
		<icon>/icon.png</icon>
		<instanceable>true</instanceable>
