<?xml version="1.0"?>

<portlet-app
	version="2.0"
	xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
>
	
	<portlet>
		<portlet-name>vclip</portlet-name>
		<display-name>VclipAction</display-name>
		<portlet-class>
			com.vportal.portlet.vclip.VclipAction
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/portlet/manage_vclip/view.jsp</value>
		</init-param>

	
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>VclipAction</title>
			<short-title>VclipAction</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>viewv</portlet-name>
		<display-name>View Vclip</display-name>
		<portlet-class>
			com.vportal.portlet.viewvclip.ViewVclipAction
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/portlet/view_vclip/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/portlet/view_vclip/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>View Vclip</title>
			<short-title>ViewVclip</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>vclipcontent</portlet-name>
		<display-name>Vclip Content</display-name>
		<portlet-class>
			com.vportal.portlet.vclipcontent.VclipContentAction
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/portlet/vclipcontent/view.jsp</value>
		</init-param>
		<init-param>
			<name>edit-jsp</name>
			<value>/html/portlet/vclipcontent/edit.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
			<portlet-mode>edit</portlet-mode>
			
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Vclip Content</title>
			<short-title>VclipContentAction</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
	<portlet>
		<portlet-name>vclipmenu</portlet-name>
		<display-name>Vclip Menu</display-name>
		<portlet-class>
			com.vportal.portlet.vclipmenu.VclipMenuAction
		</portlet-class>
		<init-param>
			<name>view-jsp</name>
			<value>/html/portlet/vclipmenu/menu.jsp</value>
		</init-param>
		<expiration-cache>0</expiration-cache>
		<supports>
			<mime-type>text/html</mime-type>
			<portlet-mode>view</portlet-mode>
		</supports>
		<resource-bundle>content/Language</resource-bundle>
		<portlet-info>
			<title>Vclip Menu</title>
			<short-title>VclipMenuAction</short-title>
			<keywords></keywords>
		</portlet-info>
		<security-role-ref>
			<role-name>administrator</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>guest</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>power-user</role-name>
		</security-role-ref>
		<security-role-ref>
			<role-name>user</role-name>
		</security-role-ref>
	</portlet>
</portlet-app>